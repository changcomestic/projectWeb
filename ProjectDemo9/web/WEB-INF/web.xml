        <servlet-class>sample.controllers.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetListCityController</servlet-name>
        <servlet-class>sample.controllers.GetListCityController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetListWayController</servlet-name>
        <servlet-class>sample.controllers.GetListWayController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetListTypeController</servlet-name>
        <servlet-class>sample.controllers.GetListTypeController</servlet-class>
